    <!-- Begin Tweet Objects -->

    <%= form_for Favorite.new do |f| %>
     <div class="panel-7">
        <i class="fa fa-twitter fa-lg" style="float:right"></i>
        <ul class="media-list">
            <li class="media">
              <a class="media-left" href="#" title="#">
                <img src="<%= @tweet_one.user.profile_image_url %>" alt="">
              </a>
              <div class="media-body">
                <h5 class="media-heading"><%= @tweet_one.user.name %> </h5>
                <h5> <a href="http://www.twitter.com/<%= @tweet_one.user.screen_name %>" target="_blank">

                    <% if current_user %>
                    <%= f.hidden_field :name, value: @tweet_one.user.name %>
                    <%= f.hidden_field :username, value: @tweet_one.user.screen_name %>
                    <%= f.hidden_field :profile_url, value: @tweet_one.user.profile_image_url %>
                    <%= f.hidden_field :text, value: @tweet_one.text %>
                    <%= f.hidden_field :icon, value: "fa-twitter" %>
                    <%= f.hidden_field :latitude, value: @lat %>
                    <%= f.hidden_field :longitude, value: @lng %>
                    <%= f.hidden_field :user_id, value: current_user.id %>
                       <% end %>

                @<%= @tweet_one.user.screen_name %></a></h5>
                <p>
                  <a href="http://www.twitter.com/<%= @tweet_one.user.screen_name %>/status/<%= @tweet_one.id %>" target="_blank">
                  <%= @tweet_one.text %>
                   </a>
                </p>
              </div>
            </li>
          </ul>
              <button class="btn btn-default btn-sm" style="float:right;">Save <i class="fa fa-star-o"></i></button>
     </div>
     <% end %>


