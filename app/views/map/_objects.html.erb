<!--Begin Instagram objects  -->
    <!-- Create form for new Favorite, populated by the hidden fields below and pulled from the Instagram object API call -->
    <div class="panel-1">
    <%= form_for Favorite.new do |f| %>
        <i class="fa fa-instagram fa-lg" style="float:right"></i>
        <a href="<%= @image_one.link %>" target="_blank" title="#">
          <img class="main" src="<%= @image_one.images.low_resolution.url %>">
          <h4>
            <img class="profile" src="<%= @image_one.user.profile_picture %>">   
            <!-- Create hidden fields with values from Instagram objects  -->
            <% if current_user %>
            <%= f.hidden_field :name, value: @image_one.user.username %>
            <%= f.hidden_field :main_url, value: @image_one.images.low_resolution.url %>
            <%= f.hidden_field :profile_url, value: @image_one.user.profile_picture %>
            <%= f.hidden_field :icon, value: "fa-instagram" %>
            <%= f.hidden_field :latitude, value: @lat %>
            <%= f.hidden_field :longitude, value: @lng %>
            <%= f.hidden_field :user_id, value: current_user.id %>
            <% end %>           
            <%= @image_one.user.username%>
          </h4>
        </a>
        <!-- Button submits hidden fields to the Favorites controller -->
        <button type="submit" class="btn btn-default btn-sm" style="float:right;">Save 
          <i class="fa fa-star-o saved"></i></button>
    <% end %>

      <div class="text-center">
              <a href="/like/<%= @image_one.id %>?address=<%= params[:address] %>" style="float:right;" remote="true">  
                <button class="btn btn-default btn-sm liked">
                Like 
                <i class="fa fa-heart"></i>
              </button>
              </a>
        </div>

      </div>


